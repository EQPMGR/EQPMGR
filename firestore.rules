rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and are for debugging only.
    // They allow any user to read and write to any part of your database.
    // We will replace these with secure rules once the "add" functionality is confirmed.
    match /{document=**} {
      allow read, write;
    }
  }
}
